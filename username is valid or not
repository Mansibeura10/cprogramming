// username is valid or not
#include<stdio.h>
#include<string.h>
void main(){
	char str1[20],str2[20];
	printf("enter a username:");
	scanf("%s",str1);
	printf("reenter a username :");
	scanf("%s",str2);
	if (strcmp(str1,str2)==0)
	printf("username is valid \n");
	else
	printf("uername is invalid ");
}
