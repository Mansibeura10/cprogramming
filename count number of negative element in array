#include <stdio.h>
#define MAX_SIZE 100 
int main()
{
    int arr[MAX_SIZE]; 
    int i, N,count=0;

    
    printf("Enter size of the array : ");
    scanf("%d", &N);

    
    printf("Enter elements in array : ");
    for(i=0; i<N; i++)
    {
        scanf("%d", &arr[i]);
    }

    printf("\nAll negative elements in array are:\n");
    for(i=0; i<N; i++)
    {
        if(arr[i]< 0)
        {
        	count+=1;
            printf("%d\n",arr[i]);
        }
    }
    printf("number of negative number in array %d\n",count);

    return 0;
}
