#include<stdio.h>
void main()
{
	int i,j,n,a[20],c=0,p=0,cp=0;
	printf("enter n vlue");
	scanf("%d",&n);
	printf("enter numbers");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<n;i++)
	{
		for(j=1;j<=n;j++)
		{
			if(a[i]%2==0)
			c=c+1;
		}
		if(c==2)
		p++;
		
		else
		cp++;
	}
	printf("prime count is %d\n",p);
	printf("composite count is =%d",cp);
}
