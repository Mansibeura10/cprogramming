//pythagoras triplet in given range

#include<stdio.h>
main(){
    int final,a,b,c;
    
    printf("\nFinal: ");
    scanf("%d",&final);
    printf("The Pythogorean Triplets in the given range are as follows:\n__\n");
    for(a=1;a<=final;a++)
	{
        for(b=a;b<=final;b++)
		{
            for(c=b;c<=final;c++)
			{
                if(c*c==a*a+b*b)
				{
                    printf("%d , %d , %d\n",a,b,c);
                }
            }
        }
    }
}
